{
  "name": "react_express_typescript_template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "start-build": "start http://localhost:3000",
    "doc": "npm run doc:typedoc && npm run doc:client",
    "doc:typedoc": "typedoc",
    "doc:client": "mkdir -p ./docs/graphs-client && npm run doc:madge:client && npm run doc:dc:client && npm run doc:dc:external:client",
    "doc:madge:client": "madge --extensions ts,tsx --ts-config tsconfig.json  --image docs/graphs-client/madge-graph-client.png client",
    "doc:dc:client": "npx depcruise client --include-only '^client' --output-type dot | dot -T svg > docs/graphs-client/dc-graph-client.svg",
    "doc:dc:external:client": "npx depcruise client --output-type dot | dot -T svg > docs/graphs-client/dc-ext-graph-client.svg",
    "clean": "npm run clean:modules && npm run clean:doc && npm run clean:dist",
    "clean:modules": "rm -r -f ./node_modules",
    "clean:dist": "rm -r -f ./dist",
    "clean:doc": "rm -r -f ./docs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@types/axios": "^0.14.4",
    "@types/bcrypt": "^5.0.2",
    "@types/dotenv": "^8.2.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-router-dom": "^5.3.3",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.2",
    "dependency-cruiser": "^16.10.0",
    "dotenv-webpack": "^8.1.0",
    "html-webpack-plugin": "^5.6.3",
    "madge": "^8.0.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.9",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.74.4",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "check-password-strength": "^3.0.0",
    "css-loader": "^7.1.2",
    "jsonwebtoken": "^9.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.5",
    "react-toastify": "^11.0.3",
    "reactjs-popup": "^2.0.6",
    "style-loader": "^4.0.0"
  }
}
